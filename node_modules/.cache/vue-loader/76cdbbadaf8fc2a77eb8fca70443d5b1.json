\n                        if (res.data.code === 0) {\r\n                            //登录成功\r\n                            console.log( '00000')\r\n                            localStorage.setItem('token',res.data.data)\r\n                            //\r\n                            this.$store.dispatch('setToken',res.data.data);\r\n                            this.$router.push({path:'/'})\r\n                        }else{\r\n                            const toast = this.$createToast({\r\n                                txt: \"登录失败\",\r\n                                type: \"error\",\r\n                                time: 1500\r\n                            });\r\n                            toast.show();\r\n                        }\r\n                    }\r\n                )\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    .main {\r\n        padding: 50px 5% 0;\r\n        text-align: center;\r\n    }\r\n    // 登录\r\n    .cube-btn {\r\n        margin-top: 20px;\r\n    }\r\n    //注册\r\n    .reg {\r\n        display: inline-block;\r\n        margin-top: 30px;\r\n        font-size: 18px;\r\n    }\r\n</style>\r\n"]}]}