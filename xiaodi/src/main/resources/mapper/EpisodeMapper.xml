<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mapper.EpisodeMapper">


    <select id="findFirstEpsiodeById" resultType="com.example.model.entity.Episode">
        select * from episode  where video_id  = #{videoId} and num =1;
    </select>

</mapper>
